{
  "name": "CozyMobile",
  "version": "3.0.0",
  "main": "src/main.jsx",
  "scripts": {
    "build": "NODE_ENV=production webpack --config webpack.config.js",
    "clean": "rm -rf www",
    "copy:manifest": "install -m0644 package.json README.md LICENSE www",
    "deploy": "git-directory-deploy --username Cozy --email contact@cozycloud.cc --directory www/ --branch build --repo=https://$GITHUB_TOKEN@github.com/cozy/cozy-mobile-v3.git",
    "tx": "tx pull --all || true",
    "postbuild": "npm run copy:manifest",
    "prebuild": "npm-run-all lint clean tx",
    "pretest": "npm run lint",
    "prewatch": "npm run clean",
    "lint": "standard 'src/**/*.js' 'src/**/*.jsx'",
    "test": "jest",
    "watch": "NODE_ENV=development webpack --config webpack.config.js --watch --display-modules --display-chunks",
    "watch:server": "NODE_ENV=development webpack-dev-server --config webpack.config.js --port 8082 --display-modules --display-chunks --inline --hot"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cozy/cozy-mobile-v3.git"
  },
  "author": "Cozy Cloud <contact@cozycloud.cc> (https://cozy.io/)",
  "contributors": [],
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/cozy/cozy-mobile-v3/issues"
  },
  "homepage": "https://github.com/cozy/cozy-mobile-v3#readme",
  "dependencies": {
    "classnames": "2.2.5",
    "cozy-ui": "3.0.0-beta1",
    "cozy-files": "https://github.com/cozy/cozy-files-v3",
    "node-polyglot": "2.2.1",
    "preact": "7.1.0",
    "preact-compat": "3.9.4",
    "react-router": "3.0.0"
  },
  "devDependencies": {
    "autoprefixer": "6.6.0",
    "babel-core": "6.21.0",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.10",
    "babel-plugin-transform-react-jsx": "6.8.0",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-polyfill": "6.20.0",
    "babel-preset-env": "1.1.4",
    "babel-preset-react": "6.16.0",
    "babel-runtime": "6.20.0",
    "chai": "3.5.0",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.26.1",
    "css-mqpacker": "5.0.1",
    "enzyme": "2.7.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "git-directory-deploy": "1.5.1",
    "html-webpack-plugin": "2.25.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "18.1.0",
    "json-loader": "0.5.4",
    "npm-run-all": "4.0.0",
    "postcss": "5.2.8",
    "postcss-discard-duplicates": "2.0.2",
    "postcss-loader": "1.2.1",
    "pug": "2.0.0-beta6",
    "pug-loader": "2.3.0",
    "react": "15.4.1",
    "react-dom": "15.4.1",
    "react-test-renderer": "15.4.1",
    "sinon": "1.17.7",
    "standard": "8.6.0",
    "style-loader": "0.13.1",
    "stylus": "0.54.5",
    "stylus-loader": "2.4.0",
    "svg-sprite-loader": "0.1.2",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2",
    "webpack-merge": "2.0.0"
  },
  "jest": {
    "moduleFileExtensions": ["js", "jsx", "json", "styl"],
    "moduleNameMapper": {
      "\\.(png|gif|jpe?g|svg)$": "<rootDir>/__mocks__/fileMock.js",
      "styles": "identity-obj-proxy"
    }
  }
}
